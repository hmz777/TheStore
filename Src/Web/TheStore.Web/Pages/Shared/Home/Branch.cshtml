@using TheStore.Web.ViewModels.Branches;
@model BranchViewModel

<article class="branch-item">
    <img class="swiper-lazy" data-src="/img/branches/@Model.Image" alt="@Model.Name" />
    <div class="branch-content">
        <h3>
            @Model.Name
        </h3>
        <p>
            @Model.Description
        </p>
    </div>
</article>